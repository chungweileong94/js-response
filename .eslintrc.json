{
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "whiteroom-base",
    "plugin:@typescript-eslint/recommended",
    "plugin:import/typescript"
  ],
  "ignorePatterns": [
    "dist",
    "node_modules"
  ],
  "rules": {
    "import/extensions": "off"
  }
}